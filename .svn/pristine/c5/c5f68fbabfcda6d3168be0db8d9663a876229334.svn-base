* {
	outline: 0;/*当元素获得焦点的时候，焦点框为0*/
}
html, body {
	height: 100%;
	font-size: 15px;
	font-family: 'Microsoft YaHei';
}
#wrap {
	height: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-flex-flow: column;
	flex-flow: column;
}
#header {
	width: 100%;
	height:40px;
	text-align: center;
	color: #fff;
	display: -webkit-box;
	background-color: #f00;
}
#header h1 {
	-webkit-box-flex: 1;
	font-size: 20px;
	height: 20px;
	line-height: 20px;
	margin: 0em;
	color: #fff;
}
#main {
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	flex: 1;
	background-color: #ff0;
}
#footer {
	height: 50px;
	line-height: 30px;
	background-color: #3C3C3C;
	width: 100%;
	text-align: center;
	color: white;
	border-top:1px solid #D4A82A;
}
#footer h5{
	color: white;
}
.show{
	display:block;
}
.hidden{
	display:none;
}
.backBar {
	display: inline-block;
	height: 30px;
	width: 30px;
	margin-top: 5px;
	margin-left: 5px;
	background-size: 30px 30px;
	background-position: center;
	background-repeat: no-repeat;
}
.backIcon {
	background-image: url("../image/backBar.png");
}
