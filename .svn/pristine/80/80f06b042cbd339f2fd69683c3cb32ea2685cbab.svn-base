		<div id="main">
			<canvas id="canvas">
				你的浏览器不支持canvas
			</canvas>
		</div>
		<div id="bluedata" >
			<input type="text" placeholder="0" id="blText1" onfocus=this.blur() />
			<input type="text"  id="blText2" onfocus=this.blur() />
			<input type="text"  id="blText3" onfocus=this.blur() />
		</div>
		<div id="footerBar">
			<div id="draw" class="footbtn">画线</div>
			<div id="linkble" class="footbtn ac">连接</div>
			<div id="backdraw" class="footbtn ac">回退</div>
			<div id="beforedraw" class="footbtn ac">前进</div>
			<div id="saveToImage" class="footbtn ac">保存</div>
		</div>
	</div>
	<div id="listMode" class="hidden">
			<div class="row"><input type="text" placeholder="名称" id="name"><div class="bottomLine active"></div></div>
			<div class="row"><input type="text" placeholder="长度" id="len"><div class="bottomLine"></div></div>
			<div class="row"><input type="text" placeholder="角度" id="angel"><div class="bottomLine"></div></div>
			<div class="row"><input type="text" placeholder="描述" id="des"><div class="bottomLine"></div></div>
			<div class="row_btn"></div>
			<div id="cancel" class="btn">取消</div>
			<div id="sure" class="btn">确定</div>
	</div>
	<div id="swip" class="hidden">	
		<ul id="spul">
			<!--<li><img src="widget://res/list.png"/></li>
			<li><img src="../iamge/rubish.png"/></li>-->
		</ul>
	</div>
	<div id="orderDiv" class="hidden">
		<ul id=orderList>
			<li id="listOpen">打开</li>
			<li id="listDel">删除</li>
			<li id="listMdName">修改名称</li>
			<li id="listMdBotPic">更换底图</li>
		</ul>
	</div>
	<div id="bgpicDiv" class="hidden">
		<div id="bgpictitle">背景图片设定</div>
		<ul id="bgpicSelect">
			<li>无</li>
			<li>照相机</li>
			<li>图片</li>
		</ul>
	</div>
